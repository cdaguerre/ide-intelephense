{
  "name": "ide-intelephense",
  "main": "./lib/ide-intelephense",
  "version": "0.4.0",
  "description": "PHP language support powered by Intelephense",
  "keywords": [],
  "activationCommands": {},
  "repository": "https://github.com/cdaguerre/ide-intelephense",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^1.6.0",
    "axios": "^0.21.1",
    "crypto": "^1.0.1",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "fs": "^0.0.1-security",
    "intelephense": "^1.6.3",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "querystring": "^0.2.1"
  },
  "enhancedScopes": [
    "text.html.php"
  ],
  "configSchema": {
    "licenseKey": {
      "title": "Intelephense license key",
      "type": "string",
      "default": "",
      "description": "Enter a license key to get access to [premium features](https://intelephense.com/)."
    },
    "globalStoragePath": {
      "title": "Global storage path",
      "type": "string",
      "default": "",
      "description": "Absolute path to a global storage dir. Defaults to `os.homedir()/.ide-intelephense`."
    },
    "storagePath": {
      "title": "Storage path",
      "type": "string",
      "default": "",
      "description": "Absolute path to storage dir. Defaults to `os.tmpdir()`."
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  }
}
